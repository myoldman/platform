        <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="sign生成器" enabled="false">
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <boolProp name="resetInterpreter">false</boolProp>
          <stringProp name="script">import javax.crypto.Mac;
import javax.crypto.spec.SecretKeySpec;
import java.security.SignatureException;

String HMAC_SHA1_ALGORITHM = &quot;HmacSHA1&quot;;
String timestamp=org.apache.commons.lang.time.DateFormatUtils.format(new java.util.Date(),&quot;yyyy-MM-dd HH:mm:ss&quot;);
sampler.addArgument(&quot;timestamp&quot;, timestamp);

java.util.TreeMap sorted_map = new java.util.TreeMap();
sorted_map.putAll(sampler.getArguments().getArgumentsAsMap());
String sign_before_hmac = &quot;&quot;;
Iterator titer=sorted_map.entrySet().iterator();
while(titer.hasNext()) {
	Map.Entry ent=(Map.Entry )titer.next();
	String key=ent.getKey().toString();
	String value=ent.getValue().toString();
	if(sign_before_hmac.length() &lt;=0) {
		sign_before_hmac = key+&quot;=&quot;+value;
	} else {
		sign_before_hmac += &quot;&amp;&quot; + key + &quot;=&quot; + value;
	}
}

String hmac_key = vars.get(&quot;APP_SECRET&quot;);
log.info(hmac_key);
log.info(sign_before_hmac);
SecretKeySpec signingKey = new SecretKeySpec(hmac_key.getBytes(), HMAC_SHA1_ALGORITHM);
Mac mac = Mac.getInstance( HMAC_SHA1_ALGORITHM );
mac.init(signingKey);
byte[] rawHmac = mac.doFinal(sign_before_hmac.getBytes());
String sign_after_hmac = new sun.misc.BASE64Encoder().encode(rawHmac);
log.info(sign_after_hmac);
sampler.addArgument(&quot;sign&quot;, sign_after_hmac);
</stringProp>
